{
  "name": "touchbar-plus",
  "main": "./lib/index.js",
  "version": "0.3.1",
  "description": "Better Atom touchbar support for Mac OSX",
  "keywords": [
    "touchbar",
    "mac",
    "osx",
    "atom"
  ],
  "homepage": "https://github.com/victorhqc/touchbar-plus",
  "repository": {
    "type": "git",
    "url": "git@github.com:victorhqc/touchbar-plus.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "scripts": {
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:octicons": "./buildocticons.sh",
    "build:all": "npm run build && npm run build:octicons",
    "electron-rebuild": "electron-rebuild --version 4.2.7 --which-module canvas --force",
    "start": "npm run build -- --watch",
    "lint": "eslint src/**/*.ts",
    "fix:mac": "export PKG_CONFIG_PATH=\"${PKG_CONFIG_PATH}:/usr/local/opt/libffi/lib/pkgconfig\"",
    "rebuild": "npm run electron-rebuild",
    "test": "echo \"TODO\"",
    "postinstall": "./postinstall.sh"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@primer/octicons": "^9.3.1",
    "color-convert": "^2.0.0",
    "emojilib": "^2.4.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "octicons": "0.0.0-882a0ed25",
    "react": "16.10.2",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "touchbar-electron-renderer": "^0.3.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@types/atom": "^1.36.1",
    "@types/color-convert": "^1.9.0",
    "@types/history": "^4.7.3",
    "@types/lodash": "^4.14.141",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.9.3",
    "@types/react-router": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "css-loader": "3.4.0",
    "electron": "^7.2.4",
    "electron-rebuild": "^1.8.8",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "fork-ts-checker-webpack-plugin": "^4.1.3",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "node-loader": "^0.6.0",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7",
    "style-loader": "1.0.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "6.2.1",
    "ts-node": "8.5.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
